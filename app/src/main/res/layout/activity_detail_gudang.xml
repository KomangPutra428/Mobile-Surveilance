<?xml version="1.0" encoding="utf-8"?>
<ScrollView xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".detail_gudang">

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent">


        <RelativeLayout
            android:id="@+id/background"
            android:layout_width="match_parent"
            android:background="@drawable/background_bluedeep"
            android:layout_height="wrap_content">

            <TextView
                android:id="@+id/textView"
                android:layout_centerInParent="true"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Profile Outlet"
                android:layout_alignParentRight="true"
                android:layout_marginRight="16dp"
                android:textStyle="bold"
                android:textColor="#fff"
                android:textSize="32dp" />

            <LinearLayout
                android:id="@+id/update"
                android:foreground="?android:attr/selectableItemBackground"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_below="@+id/textView"
                android:layout_alignParentRight="true"
                android:layout_centerHorizontal="true"
                android:layout_marginTop="80dp"
                android:layout_marginRight="22dp">
                <ImageView
                    android:layout_gravity="center"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:src="@drawable/pencill"/>
                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="Edit"
                    android:layout_marginLeft="5dp"
                    android:layout_gravity="center"
                    android:textColor="#fff"
                    android:textSize="15dp"
                    android:textStyle="bold" />
            </LinearLayout>

        </RelativeLayout>

        <com.google.android.material.card.MaterialCardView
            android:id="@+id/detail"
            android:foreground="?android:attr/selectableItemBackground"
            android:layout_width="match_parent"
            android:layout_marginTop="-16dp"
            android:layout_marginLeft="16dp"
            android:layout_below="@+id/background"
            android:layout_marginRight="16dp"
            app:cardElevation="2dp"
            app:cardCornerRadius="4dp"
            android:layout_height="wrap_content">
            <LinearLayout
                android:layout_width="match_parent"
                android:padding="15dp"
                android:orientation="vertical"
                android:layout_height="match_parent">
                <RelativeLayout
                    android:id="@+id/surveypelanggan"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="horizontal">
                    <ImageView
                        android:id="@+id/icon"
                        android:layout_width="36dp"
                        android:layout_height="36dp"
                        android:padding="4dp"
                        android:scaleType="fitCenter"
                        android:src="@drawable/credit_card" />

                    <TextView
                        android:layout_toRightOf="@+id/icon"
                        android:layout_width="wrap_content"
                        android:layout_centerInParent="true"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="16dp"
                        android:gravity="center"
                        android:textColor="#0F4C81"
                        android:textStyle="bold"
                        android:text="E-Card Pelanggan"
                        android:textAppearance="?attr/textAppearanceHeadline6"
                        android:textSize="16dp" />

                </RelativeLayout>

                <RelativeLayout
                    android:layout_marginTop="16dp"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content">
                    <TextView
                        android:text="ID Pelanggan"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:textColor="#9E9E9E"/>

                    <TextView
                        android:id="@+id/idpelanggan"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_alignParentRight="true"
                        android:textColor="#212121"
                        android:textStyle="bold" />
                </RelativeLayout>

                <RelativeLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="16dp">
                    <TextView
                        android:text="Nama Outlet"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:textColor="#9E9E9E"/>

                    <TextView
                        android:id="@+id/namaoutlet"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_alignParentRight="true"
                        android:textColor="#212121"
                        android:textStyle="bold" />
                </RelativeLayout>

                <RelativeLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="16dp">
                    <TextView
                        android:text="Segment Saat Ini"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:textColor="#9E9E9E"/>

                    <TextView
                        android:id="@+id/segment"
                        android:layout_width="200dp"
                        android:gravity="end"
                        android:layout_height="wrap_content"
                        android:layout_alignParentRight="true"
                        android:textColor="#212121"
                        android:textStyle="bold" />
                </RelativeLayout>

                <RelativeLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="16dp">
                    <TextView
                        android:text="Disarankan"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:textColor="#9E9E9E"/>

                    <TextView
                        android:id="@+id/disarankan"
                        android:layout_width="200dp"
                        android:gravity="end"
                        android:layout_height="wrap_content"
                        android:layout_alignParentRight="true"
                        android:textColor="#212121"
                        android:textStyle="bold" />
                </RelativeLayout>

                <RelativeLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="16dp">
                    <TextView
                        android:text="Pemilik Outlet"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:textColor="#9E9E9E"/>

                    <TextView
                        android:id="@+id/pemilik"
                        android:layout_width="200dp"
                        android:gravity="end"
                        android:layout_height="wrap_content"
                        android:layout_alignParentRight="true"
                        android:textColor="#212121"
                        android:textStyle="bold" />
                </RelativeLayout>
            </LinearLayout>
        </com.google.android.material.card.MaterialCardView>

        <com.google.android.material.card.MaterialCardView
            android:id="@+id/detailalamat"
            android:foreground="?android:attr/selectableItemBackground"
            android:layout_width="match_parent"
            android:layout_marginTop="24dp"
            android:layout_marginLeft="16dp"
            android:layout_below="@+id/detail"
            android:layout_marginRight="16dp"
            app:cardElevation="2dp"
            app:cardCornerRadius="4dp"
            android:layout_height="wrap_content">
            <LinearLayout
                android:layout_width="match_parent"
                android:padding="15dp"
                android:orientation="vertical"
                android:layout_height="match_parent">
                <TextView
                    android:layout_toRightOf="@+id/icon"
                    android:layout_width="wrap_content"
                    android:layout_centerInParent="true"
                    android:layout_height="wrap_content"
                    android:gravity="center"
                    android:textColor="#0F4C81"
                    android:textStyle="bold"
                    android:text="Info Alamat"
                    android:textAppearance="?attr/textAppearanceHeadline6"
                    android:textSize="16dp" />

                <RelativeLayout
                    android:layout_marginTop="16dp"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content">
                    <TextView
                        android:text="Kota"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:textColor="#9E9E9E"/>

                    <TextView
                        android:id="@+id/kota"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_alignParentRight="true"
                        android:textColor="#212121"
                        android:textStyle="bold" />
                </RelativeLayout>

                <RelativeLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="16dp">
                    <TextView
                        android:text="Kecamatan"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:textColor="#9E9E9E"/>

                    <TextView
                        android:id="@+id/kecamatan"
                        android:layout_width="250dp"
                        android:layout_height="wrap_content"
                        android:gravity="end"
                        android:layout_alignParentRight="true"
                        android:textColor="#212121"
                        android:textStyle="bold" />
                </RelativeLayout>

                <RelativeLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="16dp">
                    <TextView
                        android:text="Kelurahan"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:textColor="#9E9E9E"/>

                    <TextView
                        android:id="@+id/kelurahan"
                        android:layout_width="200dp"
                        android:gravity="end"
                        android:layout_height="wrap_content"
                        android:layout_alignParentRight="true"
                        android:textColor="#212121"
                        android:textStyle="bold" />
                </RelativeLayout>

                <RelativeLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="16dp">
                    <TextView
                        android:text="RT/RW"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:textColor="#9E9E9E"/>

                    <TextView
                        android:id="@+id/rtrw"
                        android:layout_width="200dp"
                        android:gravity="end"
                        android:layout_height="wrap_content"
                        android:layout_alignParentRight="true"
                        android:textColor="#212121"
                        android:textStyle="bold" />
                </RelativeLayout>

                <RelativeLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="16dp">
                    <TextView
                        android:text="Kode Pos"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:textColor="#9E9E9E"/>

                    <TextView
                        android:id="@+id/kodepos"
                        android:layout_width="200dp"
                        android:gravity="end"
                        android:layout_height="wrap_content"
                        android:layout_alignParentRight="true"
                        android:textColor="#212121"
                        android:textStyle="bold" />
                </RelativeLayout>

                <RelativeLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="16dp">
                    <TextView
                        android:text="Nomor Telepon"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:textColor="#9E9E9E"/>

                    <TextView
                        android:id="@+id/telepon"
                        android:layout_width="200dp"
                        android:gravity="end"
                        android:layout_height="wrap_content"
                        android:layout_alignParentRight="true"
                        android:textColor="#212121"
                        android:textStyle="bold" />
                </RelativeLayout>

                <RelativeLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="16dp">
                    <TextView
                        android:text="Mulai Berlangganan"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:textColor="#9E9E9E"/>

                    <TextView
                        android:id="@+id/langganan"
                        android:layout_width="200dp"
                        android:gravity="end"
                        android:layout_height="wrap_content"
                        android:layout_alignParentRight="true"
                        android:textColor="#212121"
                        android:textStyle="bold" />
                </RelativeLayout>

                <RelativeLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="16dp">
                    <TextView
                        android:text="Jarak Tempuh Outlet"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:textColor="#9E9E9E"/>

                    <TextView
                        android:id="@+id/jarak"
                        android:layout_width="200dp"
                        android:gravity="end"
                        android:layout_height="wrap_content"
                        android:layout_alignParentRight="true"
                        android:textColor="#212121"
                        android:textStyle="bold" />
                </RelativeLayout>

                <RelativeLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="16dp">
                    <TextView
                        android:text="Karyawan"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:textColor="#9E9E9E"/>

                    <TextView
                        android:id="@+id/karyawan"
                        android:layout_width="200dp"
                        android:gravity="end"
                        android:layout_height="wrap_content"
                        android:layout_alignParentRight="true"
                        android:textColor="#212121"
                        android:textStyle="bold" />
                </RelativeLayout>
            </LinearLayout>
        </com.google.android.material.card.MaterialCardView>

        <com.google.android.material.card.MaterialCardView
            android:id="@+id/alamat"
            android:foreground="?android:attr/selectableItemBackground"
            android:layout_width="match_parent"
            android:layout_marginTop="24dp"
            android:layout_marginLeft="16dp"
            android:layout_below="@+id/detailalamat"
            android:layout_marginRight="16dp"
            app:cardElevation="2dp"
            app:cardCornerRadius="4dp"
            android:layout_height="wrap_content">
            <LinearLayout
                android:layout_width="match_parent"
                android:padding="15dp"
                android:orientation="vertical"
                android:layout_height="match_parent">

                <LinearLayout
                    android:orientation="vertical"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content">
                    <TextView
                        android:text="Alamat"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:textColor="#000000"
                        android:textStyle="bold"/>

                    <TextView
                        android:id="@+id/alamatlengkap"
                        android:layout_marginTop="16dp"
                        android:textAllCaps="true"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:textColor="#9E9E9E" />

                    <TextView
                        android:text="Saran Perubahan"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:textColor="#000000"
                        android:layout_marginTop="15dp"
                        android:textStyle="bold"/>

                    <TextView
                        android:id="@+id/alamatdiganti"
                        android:layout_marginTop="16dp"
                        android:textAllCaps="true"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:textColor="#FF0000" />

                    <TextView
                        android:text="Catatan"
                        android:layout_marginTop="15dp"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:textColor="#000000"
                        android:textStyle="bold"/>

                    <TextView
                        android:id="@+id/catatan"
                        android:layout_marginTop="16dp"
                        android:textAllCaps="true"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:textColor="#2ECC71" />
                </LinearLayout>



            </LinearLayout>
        </com.google.android.material.card.MaterialCardView>

        <com.google.android.material.card.MaterialCardView
            android:id="@+id/infogudang"
            android:foreground="?android:attr/selectableItemBackground"
            android:layout_width="match_parent"
            android:layout_marginTop="24dp"
            android:layout_marginLeft="16dp"
            android:layout_below="@+id/alamat"
            android:layout_marginRight="16dp"
            app:cardElevation="2dp"
            app:cardCornerRadius="4dp"
            android:layout_height="wrap_content">
            <LinearLayout
                android:layout_width="match_parent"
                android:padding="15dp"
                android:orientation="vertical"
                android:layout_height="match_parent">

                <LinearLayout
                    android:orientation="vertical"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content">
                    <LinearLayout
                        android:layout_width="match_parent"
                        android:orientation="vertical"
                        android:layout_height="match_parent">
                        <TextView
                            android:layout_toRightOf="@+id/icon"
                            android:layout_width="wrap_content"
                            android:layout_centerInParent="true"
                            android:layout_height="wrap_content"
                            android:gravity="center"
                            android:textColor="#0F4C81"
                            android:textStyle="bold"
                            android:text="Info Gudang"
                            android:textAppearance="?attr/textAppearanceHeadline6"
                            android:textSize="16dp" />

                        <RelativeLayout
                            android:layout_marginTop="16dp"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content">
                            <TextView
                                android:text="Kepemilikan Gudang"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:textColor="#9E9E9E"/>

                            <TextView
                                android:id="@+id/kepemilikan"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_alignParentRight="true"
                                android:textColor="#212121"
                                android:textStyle="bold" />
                        </RelativeLayout>

                        <RelativeLayout
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_marginTop="16dp">
                            <TextView
                                android:text="Luas Gudang"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:textColor="#9E9E9E"/>

                            <TextView
                                android:id="@+id/luas"
                                android:layout_width="250dp"
                                android:layout_height="wrap_content"
                                android:gravity="end"
                                android:layout_alignParentRight="true"
                                android:textColor="#212121"
                                android:textStyle="bold" />
                        </RelativeLayout>

                        <RelativeLayout
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_marginTop="16dp">
                            <TextView
                                android:text="Panjang"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:textColor="#9E9E9E"/>

                            <TextView
                                android:id="@+id/panjang"
                                android:layout_width="200dp"
                                android:gravity="end"
                                android:layout_height="wrap_content"
                                android:layout_alignParentRight="true"
                                android:textColor="#212121"
                                android:textStyle="bold" />
                        </RelativeLayout>

                        <RelativeLayout
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_marginTop="16dp">
                            <TextView
                                android:text="Lebar"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:textColor="#9E9E9E"/>

                            <TextView
                                android:id="@+id/lebar"
                                android:layout_width="200dp"
                                android:gravity="end"
                                android:layout_height="wrap_content"
                                android:layout_alignParentRight="true"
                                android:textColor="#212121"
                                android:textStyle="bold" />
                        </RelativeLayout>

                        <RelativeLayout
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_marginTop="16dp">
                            <TextView
                                android:text="Kapasitas Galon"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:textColor="#9E9E9E"/>

                            <TextView
                                android:id="@+id/galon"
                                android:layout_width="200dp"
                                android:gravity="end"
                                android:layout_height="wrap_content"
                                android:layout_alignParentRight="true"
                                android:textColor="#212121"
                                android:textStyle="bold" />
                        </RelativeLayout>

                        <RelativeLayout
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_marginTop="16dp">
                            <TextView
                                android:text="Kapasitas SPS"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:textColor="#9E9E9E"/>

                            <TextView
                                android:id="@+id/sps"
                                android:layout_width="200dp"
                                android:gravity="end"
                                android:layout_height="wrap_content"
                                android:layout_alignParentRight="true"
                                android:textColor="#212121"
                                android:textStyle="bold" />
                        </RelativeLayout>

                        <RelativeLayout
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_marginTop="16dp">
                            <TextView
                                android:text="Jumlah Botol Sekarang"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:textColor="#9E9E9E"/>

                            <TextView
                                android:id="@+id/botol"
                                android:layout_width="200dp"
                                android:layout_height="wrap_content"
                                android:layout_alignParentRight="true"
                                android:layout_alignParentBottom="true"
                                android:layout_marginBottom="0dp"
                                android:gravity="end"
                                android:textColor="#212121"
                                android:textStyle="bold" />
                        </RelativeLayout>

                        <RelativeLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="16dp">
                        <TextView
                            android:text="Kondisi Gudang"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:textColor="#9E9E9E"/>

                        <TextView
                            android:id="@+id/kondisi"
                            android:layout_width="200dp"
                            android:gravity="end"
                            android:layout_height="wrap_content"
                            android:layout_alignParentRight="true"
                            android:textColor="#212121"
                            android:textStyle="bold" />
                    </RelativeLayout>

                        <RelativeLayout
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_marginTop="16dp">
                            <TextView
                                android:text="Ketersediaan Tempat Parkir"
                                android:layout_width="100dp"
                                android:layout_height="wrap_content"
                                android:textColor="#9E9E9E"/>

                            <TextView
                                android:id="@+id/parkir"
                                android:layout_width="200dp"
                                android:gravity="end"
                                android:layout_height="wrap_content"
                                android:layout_alignParentRight="true"
                                android:textColor="#212121"
                                android:textStyle="bold" />
                        </RelativeLayout>

                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_centerInParent="true"
                            android:layout_height="wrap_content"
                            android:gravity="center"
                            android:textColor="#000000"
                            android:textStyle="bold"
                            android:text="Chiller"
                            android:layout_marginTop="16dp"
                            />

                        <ListView
                            android:id="@+id/chillerlist"
                            android:layout_width="wrap_content"
                            android:divider="@null"
                            android:layout_height="wrap_content"/>


                    </LinearLayout>
                </LinearLayout>

            </LinearLayout>
        </com.google.android.material.card.MaterialCardView>

        <com.google.android.material.card.MaterialCardView
            android:id="@+id/pemasaran"
            android:foreground="?android:attr/selectableItemBackground"
            android:layout_width="match_parent"
            android:layout_marginTop="24dp"
            android:layout_marginLeft="16dp"
            android:layout_below="@+id/infogudang"
            android:layout_marginRight="16dp"
            app:cardElevation="2dp"
            app:cardCornerRadius="4dp"
            android:layout_height="wrap_content">
            <LinearLayout
                android:layout_width="match_parent"
                android:padding="15dp"
                android:orientation="vertical"
                android:layout_height="match_parent">

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_centerInParent="true"
                        android:layout_height="wrap_content"
                        android:textColor="#0F4C81"
                        android:textStyle="bold"
                        android:text="Pemasaran"
                        android:textAppearance="?attr/textAppearanceHeadline6"
                        android:textSize="16dp" />

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_centerInParent="true"
                    android:layout_height="wrap_content"
                    android:gravity="center"
                    android:textColor="#000000"
                    android:textStyle="bold"
                    android:text="Lokasi Sekitar Outlet"
                    android:layout_marginTop="16dp" />

                <ListView
                    android:id="@+id/lokasilist"
                    android:layout_width="wrap_content"
                    android:divider="@null"
                    android:layout_height="wrap_content"/>

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_centerInParent="true"
                    android:layout_height="wrap_content"
                    android:textColor="#000000"
                    android:textStyle="bold"
                    android:text="Cara mendapatkan order paling banyak"
                    android:layout_marginTop="16dp"/>

                <ListView
                    android:id="@+id/orderlist"
                    android:layout_width="wrap_content"
                    android:divider="@null"
                    android:layout_height="wrap_content"/>


            </LinearLayout>
