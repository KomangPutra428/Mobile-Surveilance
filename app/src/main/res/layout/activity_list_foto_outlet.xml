<?xml version="1.0" encoding="utf-8"?>
<androidx.drawerlayout.widget.DrawerLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".list_foto_outlet">
    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent">
        <ImageView
            android:id="@+id/gambardepan"
            android:visibility="gone"
            android:layout_width="226dp"
            android:layout_height="226dp"
            android:layout_marginTop="60dp"
            android:layout_centerInParent="true"
            android:src="@drawable/icon_kamera" />
        <ImageView
            android:id="@+id/gambargudang"
            android:visibility="gone"
            android:layout_width="226dp"
            android:layout_height="226dp"
            android:layout_marginTop="60dp"
            android:layout_centerInParent="true"
            android:src="@drawable/icon_kamera" />
        <ImageView
            android:id="@+id/gambarareaparkir"
            android:visibility="gone"
            android:layout_width="226dp"
            android:layout_height="226dp"
            android:layout_marginTop="60dp"
            android:layout_centerInParent="true"
            android:src="@drawable/icon_kamera" />
        <ImageView
            android:id="@+id/gambarmasuk"
            android:visibility="gone"
            android:layout_width="226dp"
            android:layout_height="226dp"
            android:layout_marginTop="60dp"
            android:layout_centerInParent="true"
            android:src="@drawable/icon_kamera" />
        <ImageView
            android:id="@+id/gambarkanan"
            android:visibility="gone"
            android:layout_width="226dp"
            android:layout_height="226dp"
            android:layout_marginTop="60dp"
            android:layout_centerInParent="true"
            android:src="@drawable/icon_kamera" />
        <ImageView
            android:id="@+id/gambarkiri"
            android:visibility="gone"
            android:layout_width="226dp"
            android:layout_height="226dp"
            android:layout_marginTop="60dp"
            android:layout_centerInParent="true"
            android:src="@drawable/icon_kamera" />
        <com.google.android.material.appbar.AppBarLayout
            android:id="@+id/toolbar"
            android:layout_width="match_parent"
            android:layout_height="56dp"
            android:elevation="8dp">

            <com.google.android.material.appbar.MaterialToolbar
                android:id="@+id/biodatabar"
                android:layout_width="match_parent"
                android:background="#ffffff"
                app:titleTextColor="#212121"
                android:layout_height="56dp"
                app:title="Foto Outlet"
                style="@style/Widget.MaterialComponents.Toolbar.Primary"
                />
        </com.google.android.material.appbar.AppBarLayout>

        <ScrollView
            android:id="@+id/profilelayout"
            android:layout_below="@+id/toolbar"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:nestedScrollingEnabled="true">
            <TableLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:stretchColumns="0,1">
                <TableRow
                    android:layout_weight="1">
                    <com.google.android.material.card.MaterialCardView
                        android:layout_marginTop="24dp"
                        android:foreground="?android:attr/selectableItemBackground"
                        android:layout_marginLeft="16dp"
                        android:layout_marginRight="16dp"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:background="@drawable/editbox">

                        <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:orientation="vertical">

                            <ImageView
                                android:id="@+id/fotodepan"
                                android:layout_width="155dp"
                                android:layout_height="226dp"
                                android:layout_gravity="center_horizontal"
                                android:src="@drawable/icon_kamera" />

                            <TextView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_marginLeft="10dp"
                                android:text="Tampak Depan"
                                android:textSize="16dp"
                                android:textStyle="bold" />

                            <TextView
                                android:id="@+id/kondisidepan"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_marginTop="15dp"
                                android:text="Kondisi"
                                android:layout_marginLeft="10dp"
                                android:textColor="#000000"
                                android:textSize="16dp"
                                android:textStyle="bold" />

                            <TextView
                                android:id="@+id/keterangandepan"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_marginTop="15dp"
                                android:layout_marginLeft="10dp"
                                android:layout_marginBottom="15dp"
                                android:text="Keterangan"
                                android:textSize="12dp"
                                android:textStyle="bold" />
                        </LinearLayout>
                    </com.google.android.material.card.MaterialCardView>

                    <com.google.android.material.card.MaterialCardView
                        android:foreground="?android:attr/selectableItemBackground"
                        android:layout_marginLeft="16dp"
                        android:layout_marginRight="16dp"
                        android:layout_marginTop="24dp"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:background="@drawable/editbox">

                        <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:orientation="vertical">

                            <ImageView
                                android:id="@+id/fotodalam"
                                android:layout_width="155dp"
                                android:layout_height="226dp"
                                android:layout_gravity="center_horizontal"
                                android:src="@drawable/icon_kamera" />

                            <TextView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:text="Dalam Gudang"
                                android:layout_marginLeft="10dp"
                                android:textSize="16dp"
                                android:textStyle="bold" />

                            <TextView
                                android:id="@+id/kondisidalam"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_marginTop="15dp"
                                android:layout_marginLeft="10dp"
                                android:text="Kondisi"
                                android:textColor="#000000"
                                android:textSize="16dp"
                                android:textStyle="bold" />

                            <TextView
                                android:id="@+id/keterangandalam"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_marginTop="15dp"
                                android:layout_marginLeft="10dp"
                                android:layout_marginBottom="15dp"
                                android:text="Keterangan"
                                android:textSize="12dp"
                                android:textStyle="bold" />
                        </LinearLayout>
                    </com.google.android.material.card.MaterialCardView>
                </TableRow>

                <TableRow
                    android:layout_weight="1"
                    android:layout_marginTop="15dp">
                    <com.google.android.material.card.MaterialCardView
                        android:foreground="?android:attr/selectableItemBackground"
                        android:layout_marginLeft="16dp"
                        android:layout_marginRight="16dp"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:background="@drawable/editbox">

                        <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:orientation="vertical">

                            <ImageView
                                android:id="@+id/fotoparkir"
                                android:layout_width="155dp"
                                android:layout_height="226dp"
                                android:layout_gravity="center_horizontal"
                                android:src="@drawable/icon_kamera" />

                            <TextView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:text="Area Parkir"
                                android:layout_marginLeft="10dp"
                                android:textSize="16dp"
                                android:textStyle="bold" />

                            <TextView
                                android:id="@+id/kondisiparkir"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_marginTop="15dp"
                                android:text="Kondisi"
                                android:layout_marginLeft="10dp"
                                android:textColor="#000000"
                                android:textSize="16dp"
                                android:textStyle="bold" />

                            <TextView
                                android:id="@+id/keteranganparkir"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_marginTop="15dp"
                                android:layout_marginLeft="10dp"
                                android:layout_marginBottom="15dp"
                                android:text="Keterangan"
                                android:textSize="12dp"
                                android:textStyle="bold" />
                        </LinearLayout>
                    </com.google.android.material.card.MaterialCardView>

                    <com.google.android.material.card.MaterialCardView
                        android:foreground="?android:attr/selectableItemBackground"
                        android:layout_marginLeft="16dp"
                        android:layout_marginRight="16dp"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:background="@drawable/editbox">

                        <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:orientation="vertical">

                            <ImageView
                                android:id="@+id/fotomasuk"
                                android:layout_width="155dp"
                                android:layout_height="226dp"
                                android:layout_gravity="center_horizontal"
                                android:src="@drawable/icon_kamera" />

                            <TextView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:text="Akses Jalan Masuk"
                                android:layout_marginLeft="10dp"
                                android:textSize="16dp"
                                android:textStyle="bold" />

                            <TextView
                                android:id="@+id/kondisimasuk"
                                android:layout_marginLeft="10dp"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_marginTop="15dp"
                                android:text="Kondisi"
                                android:textColor="#000000"
                                android:textSize="16dp"
                                android:textStyle="bold" />

                            <TextView
                                android:id="@+id/keteranganmasuk"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_marginTop="15dp"
                                android:layout_marginLeft="10dp"
                                android:layout_marginBottom="15dp"
                                android:text="Keterangan"
                                android:textSize="12dp"
                                android:textStyle="bold" />
                        </LinearLayout>
                    </com.google.android.material.card.MaterialCardView>
                </TableRow>

                <TableRow
                    android:layout_weight="1"
                    android:layout_marginTop="15dp">
                    <com.google.android.material.card.MaterialCardView
                        android:foreground="?android:attr/selectableItemBackground"
                        android:layout_marginLeft="16dp"
                        android:layout_marginRight="16dp"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:background="@drawable/editbox">

                        <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:orientation="vertical">

                            <ImageView
                                android:id="@+id/fotokanan"
                                android:layout_width="155dp"
                                android:layout_height="226dp"
                                android:layout_gravity="center_horizontal"
                                android:src="@drawable/icon_kamera" />

                            <TextView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:text="Akses Jalan Kanan"
                                android:textSize="16dp"
                                android:layout_marginLeft="10dp"
                                android:textStyle="bold" />

                            <TextView
                                android:id="@+id/kondisikanan"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_marginTop="15dp"
                                android:text="Kondisi"
                                android:layout_marginLeft="10dp"
                                android:textColor="#000000"
                                android:textSize="16dp"
                                android:textStyle="bold" />

                            <TextView
                                android:id="@+id/keterangankanan"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_marginTop="15dp"
                                android:layout_marginLeft="10dp"
                                android:layout_marginBottom="15dp"
                                android:text="Keterangan"
                                android:textSize="12dp"
                                android:textStyle="bold" />
                        </LinearLayout>
                    </com.google.android.material.card.MaterialCardView>

                    <com.google.android.material.card.MaterialCardView
                        android:foreground="?android:attr/selectableItemBackground"
                        android:layout_marginLeft="16dp"
                        android:layout_marginRight="16dp"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:background="@drawable/editbox">

                        <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:orientation="vertical">

                            <ImageView
                                android:id="@+id/fotokiri"
                                android:layout_width="155dp"
                                android:layout_height="226dp"
                                android:layout_gravity="center_horizontal"
                                android:src="@drawable/icon_kamera" />

                            <TextView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:text="Akses Jalan Kiri"
                                android:layout_marginLeft="10dp"
                                android:textSize="16dp"
                                android:textStyle="bold" />

                            <TextView
                                android:id="@+id/kondisikiri"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_marginTop="15dp"
                                android:text="Kondisi"
                                android:layout_marginLeft="10dp"
                                android:textColor="#000000"
                                android:textSize="16dp"
                                android:textStyle="bold" />

                            <TextView
                                android:id="@+id/keterangankiri"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_marginTop="15dp"
                                android:layout_marginLeft="10dp"
                                android:layout_marginBottom="15dp"
                                android:text="Keterangan"
                                android:textSize="12dp"
                                android:textStyle="bold" />
                        </LinearLayout>
                    </com.google.android.material.card.MaterialCardView>
                </TableRow>

            </TableLayout>
        </ScrollView>
    </RelativeLayout>
</androidx.drawerlayout.widget.DrawerLayout>